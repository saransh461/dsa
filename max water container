#include <iostream>
#include <vector>
using namespace std;
int main(){
    int n=9;
    int arr[]={1,8,6,2,5,4,8,3,7};
    int pro=0;
    for(int i=0;i<n;i++){
        int curr=0;
        for(int j=i+1;j<n;j++){
            if(arr[i]>=arr[j]){
                curr=(j-i)*arr[j];
                
            }
            else{
                curr=(j-i)*arr[i];
                
            }

            
        }
        pro=max(curr,pro);
    }
    cout<<pro;
    
    return 0;
}
