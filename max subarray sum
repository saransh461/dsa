#include <iostream>
#include <vector>
using namespace std;
int main(){
    int n=5;
    int arr[5]={1,2,3,4,5};
    int maxs=INT_MIN;
    for(int st=0;st<n;st++){
        int currs=0;
        for(int end=st;end<n;end++){
            currs+=arr[end];
            maxs=max(currs,maxs);
        }
    
    }
    cout<<maxs;
    return 0;
}

